{
    "name": "etna/auth-service-provider",
    "description": "Symfony 4 bundle to handle ETNA cookie based authentication",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume DUBOST",
            "email": "guillaume@etna.io"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "etna/php-rsa": "^0.2.0",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "TestApp/app/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://blu-composer.herokuapp.com"
        }
    ],
    "require-dev": {
        "behat/behat": "^3.4",
        "sensio/framework-extra-bundle": "^5.1",
        "phpunit/php-code-coverage": "^6.0",
        "phpunit/phpcov": "^5.0",
        "behat/symfony2-extension": "^2.1",
        "phploc/phploc": "^4.0",
        "etna/composer-behat-utils": "^2.1",
        "squizlabs/php_codesniffer": "^3.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "sebastian/phpcpd": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phing/phing": "^2.16",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "scripts": {
        "test": "behat",
        "behat": "behat",
        "phing": "phing",
        "coveralls": "php-coveralls"
    }
}
